{
  "name": "phoenixmatrix",
  "version": "0.1.1",
  "description": "A cross platform proxy for web developer that supports https",
  "main": "index.js",
  "scripts": {
    "start": "gulp run"
  },
  "author": "Francois Ward",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.2.15",
    "bluebird": "=2.3.11",
    "classnames": "^2.1.1",
    "connect": "~3.1.1",
    "domready": "^1.0.8",
    "electron-prebuilt": "^0.25.2",
    "flux": "^2.0.3",
    "gulp": "^3.8.10",
    "gulp-babel": "^5.1.0",
    "gulp-clean": "^0.3.1",
    "gulp-less": "^3.0.3",
    "gulp-run": "^1.6.6",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-watch": "^4.1.0",
    "keymirror": "^0.1.1",
    "lodash": "^3.0.1",
    "mkdirp": "^0.5.0",
    "node-uuid": "~1.4.1",
    "react-bootstrap": "^0.21.2",
    "run-sequence": "^1.0.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    },
    "globals": {
      "$": true
    },
    "rules": {
      "quotes": [
        1,
        "single",
        "avoid-escape"
      ],
      "new-cap": false
    },
    "plugins": [
      "react"
    ]
  },
  "devDependencies": {
    "eslint": "^0.20.0",
    "eslint-plugin-react": "^2.2.0"
  }
}
